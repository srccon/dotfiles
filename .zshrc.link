#
# ~/.zshrc
#

# If not running interactively, don't do anything
[[ $- != *i* ]] && return

# Import variables, and install 
if [[ -f $HOME/.dotfiles/.sshrc ]];then source $HOME/.dotfiles/.sshrc; else echo "Cannot find ~/.dotfiles/.sshrc, change dotfiles path"; fi

# Global Configs
for f in `ls $SHDIR/*`; do source $f; done

# Zsh Configs
for f in `ls $ZSHDIR/*`; do source $f; done
