DOTDIRNAME=".dotfiles"
DOTDIR="$HOME/$DOTDIRNAME"
DOTLIB="$DOTDIR/lib"
VIMDIR="$DOTDIR/.vim"
ALIASDIR="$DOTDIR/.alias.d"
BASHDIR="$DOTDIR/.bash.d"
ZSHDIR="$DOTDIR/.zsh.d"
INSTALLDIR="$DOTDIR/install"
SHDIR="$DOTDIR/.sh.d"

DOTORIG="$DOTDIR/orig"
DOTTMP="/tmp/dotfiles"

DOTREPO="https://github.com/srccon/dotfiles.git"

